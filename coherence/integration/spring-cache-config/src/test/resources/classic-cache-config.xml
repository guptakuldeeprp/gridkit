<?xml version="1.0"?>

<!DOCTYPE cache-config SYSTEM "cache-config.dtd">

<cache-config>
	<caching-scheme-mapping>

		<cache-mapping>
			<cache-name>test-a</cache-name>
			<scheme-name>distributed-a</scheme-name>
		</cache-mapping>

		<cache-mapping>
			<cache-name>test-b</cache-name>
			<scheme-name>distributed-a</scheme-name>
		</cache-mapping>

	</caching-scheme-mapping>

	<caching-schemes>

		<distributed-scheme>
			<scheme-name>distributed-a</scheme-name>
			<service-name>DistributedCache</service-name>
			<backing-map-scheme>
				<class-scheme>
					<class-name>com.griddynamics.coherence.test.cluster.TestHashMap</class-name>
					<init-params>
						<init-param>
							<param-type>java.lang.String</param-type>
							<param-value>a</param-value>
						</init-param>
					</init-params>
				</class-scheme>
			</backing-map-scheme>
		</distributed-scheme>

	</caching-schemes>
</cache-config>