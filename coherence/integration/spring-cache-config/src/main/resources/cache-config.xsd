<?xml version="1.0" encoding="UTF-8" ?>

<xs:schema xmlns="http://www.griddynamics.com/schema/coherence-spring" xmlns:xs="http://www.w3.org/2001/XMLSchema"
  targetNamespace="http://www.griddynamics.com/schema/coherence-spring"
    elementFormDefault="qualified"
    attributeFormDefault="unqualified">
  <xs:element name="acceptor-config">
    <xs:complexType>
      <xs:sequence>
        <xs:choice>
          <xs:element ref="jms-acceptor" />
          <xs:element ref="tcp-acceptor" />
        </xs:choice>
        <xs:element ref="outgoing-message-handler" minOccurs="0" />
        <xs:element ref="use-filters" minOccurs="0" />
        <xs:element ref="serializer" minOccurs="0" />
        <xs:element ref="connection-limit" minOccurs="0" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="address">
    <xs:complexType mixed="true">
      <xs:attribute name="system-property" type="xs:string" use="optional" />
    </xs:complexType>
  </xs:element>

  <xs:element name="address-provider">
    <xs:complexType>
      <xs:sequence>
        <xs:choice>
          <xs:element ref="class-name" />
          <xs:sequence>
            <xs:element ref="class-factory-name" />
            <xs:element ref="method-name" />
          </xs:sequence>
        </xs:choice>
        <xs:element ref="init-params" minOccurs="0" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="async">
    <xs:complexType mixed="true" />
  </xs:element>

  <xs:element name="async-limit">
    <xs:complexType mixed="true" />
  </xs:element>

  <xs:element name="async-store-manager">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="class-name" minOccurs="0" />
        <xs:element ref="init-params" minOccurs="0" />
        <xs:choice>
          <xs:element ref="custom-store-manager" />
          <xs:element ref="lh-file-manager" />
          <xs:element ref="bdb-store-manager" />
          <xs:element ref="nio-file-manager" />
          <xs:element ref="nio-memory-manager" />
        </xs:choice>
        <xs:element ref="async-limit" minOccurs="0" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="authorized-hosts">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="host-address" minOccurs="0" maxOccurs="unbounded" />
        <xs:element ref="host-range" minOccurs="0" maxOccurs="unbounded" />
        <xs:element ref="host-filter" minOccurs="0" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="auto-adjust">
    <xs:complexType mixed="true" />
  </xs:element>

  <xs:element name="autostart">
    <xs:complexType mixed="true">
      <xs:attribute name="system-property" type="xs:string" use="optional" />
    </xs:complexType>
  </xs:element>

  <xs:element name="back-scheme">
    <xs:complexType>
      <xs:choice>
        <xs:element ref="distributed-scheme" />
        <xs:element ref="replicated-scheme" />
        <xs:element ref="optimistic-scheme" />
        <xs:element ref="local-scheme" />
        <xs:element ref="disk-scheme" />
        <xs:element ref="external-scheme" />
        <xs:element ref="paged-external-scheme" />
        <xs:element ref="overflow-scheme" />
        <xs:element ref="class-scheme" />
        <xs:element ref="remote-cache-scheme" />
      </xs:choice>
    </xs:complexType>
  </xs:element>

  <xs:element name="backing-map-scheme">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="partitioned" minOccurs="0" />
        <xs:choice>
          <xs:element ref="local-scheme" />
          <xs:element ref="disk-scheme" />
          <xs:element ref="external-scheme" />
          <xs:element ref="paged-external-scheme" />
          <xs:element ref="overflow-scheme" />
          <xs:element ref="class-scheme" />
          <xs:element ref="overflow-scheme" />
          <xs:element ref="read-write-backing-map-scheme" />
          <xs:element ref="versioned-backing-map-scheme" />
        </xs:choice>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="backup-count">
    <xs:complexType mixed="true" />
  </xs:element>

  <xs:element name="backup-count-after-writebehind">
    <xs:complexType mixed="true" />
  </xs:element>

  <xs:element name="backup-storage">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="type" />
        <xs:choice>
          <xs:sequence>
            <xs:element ref="initial-size" minOccurs="0" />
            <xs:element ref="maximum-size" minOccurs="0" />
            <xs:element ref="directory" minOccurs="0" />
          </xs:sequence>
          <xs:element ref="class-name" minOccurs="0" />
          <xs:element ref="scheme-name" minOccurs="0" />
        </xs:choice>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="bdb-store-manager">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="class-name" minOccurs="0" />
        <xs:element ref="init-params" minOccurs="0" />
        <xs:element ref="directory" minOccurs="0" />
        <xs:element ref="store-name" minOccurs="0" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="bundle-config">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="operation-name" />
        <xs:element ref="preferred-size" minOccurs="0" />
        <xs:element ref="delay-millis" minOccurs="0" />
        <xs:element ref="thread-threshold" minOccurs="0" />
        <xs:element ref="auto-adjust" minOccurs="0" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="cache-config">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="caching-scheme-mapping" />
        <xs:element ref="caching-schemes" />
      </xs:sequence>
      <xs:attribute name="id" type="xs:string" use="required"/>
    </xs:complexType>
  </xs:element>

  <xs:element name="cache-mapping">
    <xs:complexType>
        <xs:sequence>
        <xs:element ref="cache-name" />
        <xs:element ref="scheme-name" />
        <xs:element ref="init-params" minOccurs="0" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="cache-name">
    <xs:complexType mixed="true" />
  </xs:element>

  <xs:element name="cache-name-suffix">
    <xs:complexType mixed="true" />
  </xs:element>

  <xs:element name="cache-service-proxy">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="enabled" minOccurs="0" />
        <xs:element ref="lock-enabled" minOccurs="0" />
        <xs:element ref="read-only" minOccurs="0" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="cachestore-scheme">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="scheme-name" minOccurs="0" />
        <xs:element ref="scheme-ref" minOccurs="0" />
        <xs:choice>
          <xs:element ref="class-scheme" />
          <xs:element ref="remote-cache-scheme" />
        </xs:choice>
        <xs:element ref="operation-bundling" minOccurs="0" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="cachestore-timeout">
    <xs:complexType mixed="true" />
  </xs:element>

  <xs:element name="caching-scheme-mapping">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="cache-mapping" maxOccurs="unbounded" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="caching-schemes">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="distributed-scheme" minOccurs="0"/>
        <xs:element ref="replicated-scheme" minOccurs="0"/>
        <xs:element ref="optimistic-scheme" minOccurs="0"/>
        <xs:element ref="local-scheme" minOccurs="0"/>
        <xs:element ref="disk-scheme" minOccurs="0"/>
        <xs:element ref="external-scheme" minOccurs="0"/>
        <xs:element ref="paged-external-scheme" minOccurs="0"/>
        <xs:element ref="overflow-scheme" minOccurs="0"/>
        <xs:element ref="class-scheme" minOccurs="0"/>
        <xs:element ref="near-scheme" minOccurs="0"/>
        <xs:element ref="versioned-near-scheme" minOccurs="0"/>
        <xs:element ref="overflow-scheme" minOccurs="0"/>
        <xs:element ref="invocation-scheme" minOccurs="0"/>
        <xs:element ref="read-write-backing-map-scheme" minOccurs="0"/>
        <xs:element ref="versioned-backing-map-scheme" minOccurs="0"/>
        <xs:element ref="remote-invocation-scheme" minOccurs="0"/>
        <xs:element ref="proxy-scheme" minOccurs="0"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="class-factory-name">
    <xs:complexType mixed="true" />
  </xs:element>

  <xs:element name="class-name">
    <xs:complexType mixed="true" />
  </xs:element>

  <xs:element name="class-scheme">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="scheme-name" minOccurs="0" />
        <xs:element ref="scheme-ref" minOccurs="0" />
        <!-- >xs:choice>
          <xs:element ref="class-name" />
          <xs:sequence>
            <xs:element ref="class-factory-name" />
            <xs:element ref="method-name" />
          </xs:sequence>
        </xs:choice-->
        <xs:element ref="init-params" minOccurs="0" />
        <xs:element ref="listener" minOccurs="0" />
      </xs:sequence >
    </xs:complexType>
  </xs:element>

  <xs:element name="connect-timeout">
    <xs:complexType mixed="true" />
  </xs:element>

  <xs:element name="connection-limit">
    <xs:complexType mixed="true" />
  </xs:element>

  <xs:element name="custom-store-manager">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="class-name" />
        <xs:element ref="init-params" minOccurs="0" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="delay-millis">
    <xs:complexType mixed="true" />
  </xs:element>

  <xs:element name="directory">
    <xs:complexType mixed="true">
      <xs:attribute name="system-property" type="xs:string" use="optional" />
    </xs:complexType>
  </xs:element>

  <xs:element name="disk-scheme">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="scheme-name" minOccurs="0" />
        <xs:element ref="scheme-ref" minOccurs="0" />
        <xs:element ref="class-name" minOccurs="0" />
        <xs:element ref="init-params" minOccurs="0" />
        <xs:element ref="file-manager" minOccurs="0" />
        <xs:choice>
          <xs:element ref="root-directory" />
          <xs:element ref="directory" />
        </xs:choice>
        <xs:element ref="file-name" minOccurs="0" />
        <xs:element ref="high-units" minOccurs="0" />
        <xs:element ref="initial-size" minOccurs="0" />
        <xs:element ref="maximum-size" minOccurs="0" />
        <xs:element ref="page-limit" minOccurs="0" />
        <xs:element ref="page-duration" minOccurs="0" />
        <xs:element ref="listener" minOccurs="0" />
        <xs:element ref="async" minOccurs="0" />
        <xs:element ref="async-limit" minOccurs="0" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="distributed-scheme">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="scheme-name" minOccurs="0" />
        <xs:element ref="scheme-ref" minOccurs="0" />
        <xs:element ref="service-name" minOccurs="0" />
        <xs:element ref="serializer" minOccurs="0" />
        <xs:element ref="thread-count" minOccurs="0" />
        <xs:element ref="standard-lease-milliseconds" minOccurs="0" />
        <xs:element ref="lease-granularity" minOccurs="0" />
        <xs:element ref="local-storage" minOccurs="0" />
        <xs:element ref="partition-count" minOccurs="0" />
        <xs:element ref="transfer-threshold" minOccurs="0" />
        <xs:element ref="backup-count" minOccurs="0" />
        <xs:element ref="backup-count-after-writebehind" minOccurs="0" />
        <xs:element ref="backup-storage" minOccurs="0" />
        <xs:element ref="key-associator" minOccurs="0" />
        <xs:element ref="key-partitioning" minOccurs="0" />
        <xs:element ref="partition-listener" minOccurs="0" />
        <xs:element ref="task-hung-threshold" minOccurs="0" />
        <xs:element ref="task-timeout" minOccurs="0" />
        <xs:element ref="request-timeout" minOccurs="0" />
        <xs:element ref="guardian-timeout" minOccurs="0" />
        <xs:element ref="operation-bundling" minOccurs="0" />
        <xs:element ref="backing-map-scheme" minOccurs="0" />
        <xs:element ref="listener" minOccurs="0" />
        <xs:element ref="autostart" minOccurs="0" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="enabled">
    <xs:complexType mixed="true" />
  </xs:element>

  <xs:element name="eviction-policy">
    <xs:complexType mixed="true">
      <xs:choice>
        <xs:element ref="class-scheme" />
      </xs:choice>
    </xs:complexType>
  </xs:element>

  <xs:element name="expiry-delay">
    <xs:complexType mixed="true" />
  </xs:element>

  <xs:element name="expiry-enabled">
    <xs:complexType mixed="true" />
  </xs:element>

  <xs:element name="external-scheme">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="scheme-name" minOccurs="0" />
        <xs:element ref="scheme-ref" minOccurs="0" />
        <xs:element ref="class-name" minOccurs="0" />
        <xs:element ref="init-params" minOccurs="0" />
        <xs:choice>
          <xs:element ref="async-store-manager" />
          <xs:choice>
            <xs:element ref="custom-store-manager" />
            <xs:element ref="lh-file-manager" />
            <xs:element ref="bdb-store-manager" />
            <xs:element ref="nio-file-manager" />
            <xs:element ref="nio-memory-manager" />
          </xs:choice>
        </xs:choice>
        <xs:element ref="high-units" minOccurs="0" />
        <xs:element ref="unit-calculator" minOccurs="0" />
        <xs:element ref="unit-factor" minOccurs="0" />
        <xs:element ref="expiry-delay" minOccurs="0" />
        <xs:element ref="listener" minOccurs="0" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="file-manager">
    <xs:complexType mixed="true" />
  </xs:element>

  <xs:element name="file-name">
    <xs:complexType mixed="true" />
  </xs:element>

  <xs:element name="filter-name">
    <xs:complexType mixed="true" />
  </xs:element>

  <xs:element name="flush-delay">
    <xs:complexType mixed="true" />
  </xs:element>

  <xs:element name="from-address">
    <xs:complexType mixed="true" />
  </xs:element>

  <xs:element name="front-scheme">
    <xs:complexType>
      <xs:choice>
        <xs:element ref="local-scheme" />
        <xs:element ref="disk-scheme" />
        <xs:element ref="external-scheme" />
        <xs:element ref="paged-external-scheme" />
        <xs:element ref="overflow-scheme" />
        <xs:element ref="class-scheme" />
      </xs:choice>
    </xs:complexType>
  </xs:element>

  <xs:element name="guardian-timeout">
    <xs:complexType mixed="true" />
  </xs:element>

  <xs:element name="heartbeat-interval">
    <xs:complexType mixed="true" />
  </xs:element>

  <xs:element name="heartbeat-timeout">
    <xs:complexType mixed="true" />
  </xs:element>

  <xs:element name="high-units">
    <xs:complexType mixed="true" />
  </xs:element>

  <xs:element name="host-address">
    <xs:complexType mixed="true" />
  </xs:element>

  <xs:element name="host-filter">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="class-name" />
        <xs:element ref="init-params" minOccurs="0" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="host-range">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="from-address" />
        <xs:element ref="to-address" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="init-param">
    <xs:complexType>
      <xs:sequence>
        <xs:choice>
          <xs:element ref="param-name" />
          <xs:element ref="param-type" />
        </xs:choice>
        <xs:element ref="param-value" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="init-params">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="init-param" minOccurs="0" maxOccurs="unbounded" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="initial-size">
    <xs:complexType mixed="true" />
  </xs:element>

  <xs:element name="initiator-config">
    <xs:complexType>
      <xs:sequence>
        <xs:choice>
          <xs:element ref="jms-initiator" />
          <xs:element ref="tcp-initiator" />
        </xs:choice>
        <xs:element ref="outgoing-message-handler" minOccurs="0" />
        <xs:element ref="use-filters" minOccurs="0" />
        <xs:element ref="serializer" minOccurs="0" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="internal-cache-scheme">
    <xs:complexType>
      <xs:choice>
        <xs:element ref="local-scheme" />
        <xs:element ref="disk-scheme" />
        <xs:element ref="external-scheme" />
        <xs:element ref="paged-external-scheme" />
        <xs:element ref="overflow-scheme" />
        <xs:element ref="class-scheme" />
      </xs:choice>
    </xs:complexType>
  </xs:element>

  <xs:element name="invalidation-strategy">
    <xs:complexType mixed="true" />
  </xs:element>

  <xs:element name="invocation-scheme">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="scheme-name" minOccurs="0" />
        <xs:element ref="scheme-ref" minOccurs="0" />
        <xs:element ref="service-name" minOccurs="0" />
        <xs:element ref="serializer" minOccurs="0" />
        <xs:element ref="thread-count" minOccurs="0" />
        <xs:element ref="task-hung-threshold" minOccurs="0" />
        <xs:element ref="task-timeout" minOccurs="0" />
        <xs:element ref="request-timeout" minOccurs="0" />
        <xs:element ref="guardian-timeout" minOccurs="0" />
        <xs:element ref="autostart" minOccurs="0" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="invocation-service-proxy">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="enabled" minOccurs="0" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="jms-acceptor">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="queue-connection-factory-name" />
        <xs:element ref="queue-name" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="jms-initiator">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="queue-connection-factory-name" />
        <xs:element ref="queue-name" />
        <xs:element ref="connect-timeout" minOccurs="0" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="keep-alive-enabled">
    <xs:complexType mixed="true" />
  </xs:element>

  <xs:element name="key-associator">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="class-name" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="key-partitioning">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="class-name" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="lease-granularity">
    <xs:complexType mixed="true" />
  </xs:element>

  <xs:element name="lh-file-manager">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="class-name" minOccurs="0" />
        <xs:element ref="init-params" minOccurs="0" />
        <xs:element ref="directory" minOccurs="0" />
        <xs:element ref="file-name" minOccurs="0" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="linger-timeout">
    <xs:complexType mixed="true" />
  </xs:element>

  <xs:element name="listen-backlog">
    <xs:complexType mixed="true" />
  </xs:element>

  <xs:element name="listener">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="class-scheme" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="local-address">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="address" minOccurs="0" />
        <xs:element ref="port" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="local-scheme">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="scheme-name" minOccurs="0" />
        <xs:element ref="scheme-ref" minOccurs="0" />
        <xs:element ref="class-name" minOccurs="0" />
        <xs:element ref="service-name" minOccurs="0" />
        <xs:element ref="init-params" minOccurs="0" />
        <xs:element ref="eviction-policy" minOccurs="0" />
        <xs:element ref="high-units" minOccurs="0" />
        <xs:element ref="low-units" minOccurs="0" />
        <xs:element ref="unit-calculator" minOccurs="0" />
        <xs:element ref="unit-factor" minOccurs="0" />
        <xs:element ref="expiry-delay" minOccurs="0" />
        <xs:element ref="flush-delay" minOccurs="0" />
        <xs:element ref="cachestore-scheme" minOccurs="0" />
        <xs:element ref="pre-load" minOccurs="0" />
        <xs:element ref="listener" minOccurs="0" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="local-storage">
    <xs:complexType mixed="true">
      <xs:attribute name="system-property" type="xs:string" use="optional" />
    </xs:complexType>
  </xs:element>

  <xs:element name="lock-enabled">
    <xs:complexType mixed="true" />
  </xs:element>

  <xs:element name="low-units">
    <xs:complexType mixed="true" />
  </xs:element>

  <xs:element name="manage-transient">
    <xs:complexType mixed="true" />
  </xs:element>

  <xs:element name="maximum-size">
    <xs:complexType mixed="true" />
  </xs:element>

  <xs:element name="method-name">
    <xs:complexType mixed="true" />
  </xs:element>

  <xs:element name="miss-cache-scheme">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="local-scheme" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="near-scheme">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="scheme-name" minOccurs="0" />
        <xs:element ref="scheme-ref" minOccurs="0" />
        <xs:element ref="class-name" minOccurs="0" />
