package org.apteka.insurance.rule

import org.apteka.insurance.model.Client
import org.apteka.insurance.model.Accumulator
import org.apteka.insurance.model.Drug

global org.apteka.insurance.service.cache.CacheService cacheService;

rule "Your Second Rule"
	when
		a : Accumulator()
		d : Drug()
		eval(a.getDrugLimits().get(d.getName()) > 0)
	then 
		a.getDrugLimits().put()
end
